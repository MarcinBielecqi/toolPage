{
	"_version": "1.8.0",
	"sap.app": {
		"id": "TestApp.TestApp",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "TestApp.TestApp.view.Master",
			"type": "JS",
			"async": true,
			"id": "Master"
		},
		"dependencies": {
			"minUI5Version": "1.60.1",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.tnt": {},
				"sap.ui.layout": {},
				"sap.uxap": {},
				"sap.suite.ui.microchart": {
					"lazy": true
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "TestApp.TestApp.i18n.i18n"
				}
			},
			"side": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/sideContent.json"
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "JS",
				"async": true,
				"viewPath": "TestApp.TestApp.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "RouteMaster",
				"pattern": "RouteMaster",
				"target": [
					"TargetMaster"
				]
			}, {
				"pattern": "BlockLayout",
				"name": "blockLayout",
				"target": "BlockLayout"
			}, {
				"pattern": "Page3",
				"name": "page3",
				"target": "Page3"
			}, {
				"pattern": "Page2",
				"name": "page2",
				"target": "Page2"
			}],
			"targets": {
				"TargetMaster": {
					"viewType": "JS",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "Master",
					"viewName": "Master"
				},
				"BlockLayout": {
					"viewId": "BlockLayout",
					"viewName": "BlockLayout",
					"controlId": "appPages",
					"controlAggregation": "pages"
				},
				"Page2": {
					"viewType": "JS",
					"viewName": "Page2",
					"controlId": "appPages",
					"controlAggregation": "pages"
				},
				"Page3": {
					"viewType": "JS",
					"viewName": "Page3",
					"controlId": "appPages",
					"controlAggregation": "pages"
				}
			}
		}
	}
}